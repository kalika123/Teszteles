        <dependency id="LightBDD.Core" version="2.1.0" />
      </group>
      <group targetFramework=".NETFramework4.6">
        <dependency id="LightBDD.Core" version="2.1.0" />
      </group>
      <group targetFramework="Unsupported0.0">
        <dependency id="NETStandard.Library" version="1.6.0" />
        <dependency id="LightBDD.Core" version="2.1.0" />
      </group>
    </dependencies>
  </metadata>
